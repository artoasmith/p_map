                            <g>
                                <circle class="st0" cx="64.9" cy="67.4" r="63.2"/>
                                <linearGradient id="SVGID_1_" gradientUnits="userSpaceOnUse" x1="64.9198" y1="63.4896" x2="64.9198" y2="0">
                                    <stop  offset="0" style="stop-color:#D24A43"/>
                                    <stop  offset="1" style="stop-color:#C2463E"/>
                                </linearGradient>
                                <path class="st1" d="M129.8,63.5C129.8,28.4,100.8,0,64.9,0S0,28.4,0,63.5H129.8z"/>
                                <linearGradient id="SVGID_2_" gradientUnits="userSpaceOnUse" x1="64.9198" y1="133.0336" x2="64.9198" y2="69.544">
                                    <stop  offset="0" style="stop-color:#C8C8C8"/>
                                    <stop  offset="1" style="stop-color:#E5E5E5"/>
                                </linearGradient>
                                <path class="st2" d="M0,69.5C0,104.6,29.1,133,64.9,133s64.9-28.4,64.9-63.5H0z"/>
                                <circle class="st0" cx="64.9" cy="67.4" r="21.7"/>
                                <circle class="st3" cx="64.9" cy="67.6" r="13.1"/>
                                <linearGradient id="SVGID_4_" gradientUnits="userSpaceOnUse" x1="64.9198" y1="78.4885" x2="64.9198" y2="56.6309">
                                    <stop  offset="0" style="stop-color:#C8C8C8"/>
                                    <stop  offset="1" style="stop-color:#E5E5E5"/>
                                </linearGradient>
                                <circle class="st4" cx="64.9" cy="67.6" r="10.9"/>
                            </g>
                        </svg>
                        <!-- svg -->   
                        <a href="" class="top">
                            <span>LOG IN</span>
                        </a>
                        <a href="" class="bot">
                            <span>REGISTER</span>
                        </a>
                    </div>
                </div>
            </div>
        </div>

        {% block banner %}

        {% endblock %}
        {% block content %}

        {% endblock %}
    </div>
    <!-- /MAIN -->
    {% block footerNav %}
        <div class="header-wrap">
            <div class="mbox">
                <div class="con">
                    <div class="logo-block">
                        {% if logo.image is defined %}
                            <div class="logo">
                                <img src="{{ logo.image }}" alt="">
                            </div>
                        {% endif %}
                        {% if logo.text is defined %}
                            <div class="logo-text">{{ logo.text }}</div>
                        {% endif %}
                    </div>
                    <div class="nav-part">
                        {% if main_menu is defined and main_menu is not empty %}
                            <ul>
                                {% for menu in main_menu %}
                                    <li>
                                        <a href="{{ menu.link }}">{{ menu.title }}</a>
                                    </li>
                                {% endfor %}
                            </ul>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    {% endblock %}

    <div class="hidden-block">
        {% block hidden_block %}

        {% endblock %}
    </div>

    {% block footer %}
        <!-- FOOTER -->
        <div class="footer_placeholder"></div>
        <footer class="footer">
            <div class="row">
                {% if copi_right is not empty %}
                    &copy; {{ copi_right | raw }}
                {% endif %}
            </div>
        </footer>
        <!-- FOOTER -->
    {% endblock %}
    {% block javascript %}
        <!-- SCRIPTS -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
        <script type="text/javascript" src="/js/validate_script.js" ></script>
 <!--             
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
 -->
    

        <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBBPqB8Xg3YCcmxmVah8AO5mZTXVXc2LeY"></script>
 
        
        <!-- build:js -->
        <script type="text/javascript" src="/js/plagins/device.js" ></script>
        <script type="text/javascript" src="/js/plagins/jquery.fancybox.pack.js" ></script>
        <script type="text/javascript" src="/js/plagins/jquery.formstyler.min.js" ></script>
        <script type="text/javascript" src="/js/plagins/jquery.validate.min.js" ></script>
        <script type="text/javascript" src="/js/plagins/maskInput.js" ></script>
        <script type="text/javascript" src="/js/plagins/slick.js" ></script>

        <script type="text/javascript" src="/js/basic_scripts.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_1.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_2.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_4.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_5.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_6.js" ></script>
        <!-- endbuild -->
        <!-- /SCRIPTS -->
    {% endblock %}
</div>
</body>
</html>