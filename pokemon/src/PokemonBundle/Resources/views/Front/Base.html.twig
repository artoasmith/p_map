<!DOCTYPE html>
<html lang="ru">
<head>
    {% block metaData %}
        <meta charset="UTF-8">
        {% if page_title is defined %}
            <title>{{ page_title }}</title>
        {% else %}
            <title>Pok√©mon</title>
        {% endif %}
        <meta id="viewport" name="viewport" content="width=1280,user-scalable=yes">
        <meta name="format-detection" content="telephone=no">
        <meta name="MobileOptimized" content="1280"/>
        <meta name="HandheldFriendly" content="false"/>

        <meta name="format-detection" content="telephone=no">
    {% endblock %}
    {% block stylesheets %}
        <!-- build:css -->

        <link rel="stylesheet" href="/css/fancybox.css">
        <link rel="stylesheet" href="/css/formstyler.css">
        <link rel="stylesheet" href="/css/normalize.css">
        <link rel="stylesheet" href="/css/slick-theme.css">
        <link rel="stylesheet" href="/css/slick.css">
        <!-- add new file here -->

        <link rel="stylesheet" href="/css/zdev_0_basic.css">
        <link rel="stylesheet" href="/css/zdev_1.css">
        <link rel="stylesheet" href="/css/zdev_1_adapt.css">
        <link rel="stylesheet" href="/css/zdev_2.css">
        <link rel="stylesheet" href="/css/zdev_2_adapt.css">
        <link rel="stylesheet" href="/css/zdev_4.css">
        <link rel="stylesheet" href="/css/zdev_4_adapt.css">
        <link rel="stylesheet" href="/css/zdev_5.css">
        <link rel="stylesheet" href="/css/zdev_5_adapt.css">
        <link rel="stylesheet" href="/css/zdev_6.css">
        <link rel="stylesheet" href="/css/zdev_6_adapt.css">
        <!-- endbuild -->

        <!--[if lt IE 10]>
        <link rel="stylesheet" href="https://rawgit.com/codefucker/finalReject/master/reject/reject.css" media="all" />
        <script type="text/javascript" src="https://rawgit.com/codefucker/finalReject/master/reject/reject.min.js"></script>
        <![endif]-->
        <!--[if lt IE 9]>
        <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    {% endblock %}
</head>
<body>

<div class="global-wrapper">
    {% block header %}
        <!-- HEADER -->
        <header>
            <div class="header-wrap">
                <div class="mbox">
                    <div class="con">
                        <div class="logo-block">
                            {% if logo.image is defined %}
                                <div class="logo">
                                    <img src="{{ logo.image }}" alt="">
                                </div>
                            {% endif %}
                            {% if logo.text is defined %}
                                <div class="logo-text">{{ logo.text }}</div>
                            {% endif %}
                        </div>
                        <div class="nav-part">
                            {% if main_menu is defined and main_menu is not empty %}
                            <ul>
                                {% for menu in main_menu %}
                                <li>
                                    <a href="{{ menu.link }}">{{ menu.title }}</a>
                                </li>
                                {% endfor %}
                            </ul>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- /HEADER -->
    {% endblock %}
    <!-- MAIN -->
    <div class="main">
        <!-- add partials here -->
        {% block banner %}
            {% if banner is defined %}
                <div class="banner-wrap">
                    <div class="contein">
                        <img src="{{ banner }}" alt="">
                    </div>
                </div>
            {% endif %}
        {% endblock %}
        {% block content %}

        {% endblock %}
    </div>
    <!-- /MAIN -->
    {% block footerNav %}
        <div class="header-wrap">
            <div class="mbox">
                <div class="con">
                    <div class="logo-block">
                        {% if logo.image is defined %}
                            <div class="logo">
                                <img src="{{ logo.image }}" alt="">
                            </div>
                        {% endif %}
                        {% if logo.text is defined %}
                            <div class="logo-text">{{ logo.text }}</div>
                        {% endif %}
                    </div>
                    <div class="nav-part">
                        {% if main_menu is defined and main_menu is not empty %}
                            <ul>
                                {% for menu in main_menu %}
                                    <li>
                                        <a href="{{ menu.link }}">{{ menu.title }}</a>
                                    </li>
                                {% endfor %}
                            </ul>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    {% endblock %}

    <div class="hidden-block">
        {% block hidden_block %}

        {% endblock %}
    </div>

    {% block footer %}
        <!-- FOOTER -->
        <div class="footer_placeholder"></div>
        <footer class="footer">
            <div class="row">
                {% if copi_right is not empty %}
                    &copy; {{ copi_right | raw }}
                {% endif %}
            </div>
        </footer>
        <!-- FOOTER -->
    {% endblock %}
    {% block javascript %}
        <!-- SCRIPTS -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
        <script type="text/javascript" src="/js/validate_script.js" ></script>
        <!--
        <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBBPqB8Xg3YCcmxmVah8AO5mZTXVXc2LeY&callback=initMap"></script>
        -->
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>

        
        <!-- build:js -->
        <script type="text/javascript" src="/js/plagins/device.js" ></script>
        <script type="text/javascript" src="/js/plagins/jquery.fancybox.pack.js" ></script>
        <script type="text/javascript" src="/js/plagins/jquery.formstyler.min.js" ></script>
        <script type="text/javascript" src="/js/plagins/jquery.validate.min.js" ></script>
        <script type="text/javascript" src="/js/plagins/maskInput.js" ></script>
        <script type="text/javascript" src="/js/plagins/slick.js" ></script>

        <script type="text/javascript" src="/js/basic_scripts.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_1.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_2.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_4.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_5.js" ></script>
        <script type="text/javascript" src="/js/develop/develop_6.js" ></script>
        <!-- endbuild -->
        <!-- /SCRIPTS -->
    {% endblock %}
</div>
</body>
</html>