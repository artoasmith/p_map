<!DOCTYPE html>
<html lang="ru">
<head>
    {% block metaData %}
        <meta charset="UTF-8">
        {% if page_title is defined %}
            <title>{{ page_title }}</title>
        {% else %}
            <title>Pokémon</title>
        {% endif %}
        <meta id="viewport" name="viewport" content="width=1280,user-scalable=yes">
        <meta name="format-detection" content="telephone=no">
        <meta name="MobileOptimized" content="1280"/>
        <meta name="HandheldFriendly" content="false"/>

        <meta name="format-detection" content="telephone=no">
    {% endblock %}
    {% block stylesheets %}
        <!-- build:css -->

        <link rel="stylesheet" href="/css/fancybox.css">
        <link rel="stylesheet" href="/css/formstyler.css">
        <link rel="stylesheet" href="/css/normalize.css">
        <link rel="stylesheet" href="/css/slick-theme.css">
        <link rel="stylesheet" href="/css/slick.css">
        <!-- add new file here -->

        <link rel="stylesheet" href="/css/zdev_0_basic.css">
        <link rel="stylesheet" href="/css/zdev_1.css">
        <link rel="stylesheet" href="/css/zdev_1_adapt.css">
        <link rel="stylesheet" href="/css/zdev_2.css">
        <link rel="stylesheet" href="/css/zdev_2_adapt.css">
        <link rel="stylesheet" href="/css/zdev_4.css">
        <link rel="stylesheet" href="/css/zdev_4_adapt.css">
        <link rel="stylesheet" href="/css/zdev_5.css">
        <link rel="stylesheet" href="/css/zdev_5_adapt.css">
        <link rel="stylesheet" href="/css/zdev_6.css">
        <link rel="stylesheet" href="/css/zdev_6_adapt.css">
        <!-- endbuild -->

        <!--[if lt IE 10]>
        <link rel="stylesheet" href="https://rawgit.com/codefucker/finalReject/master/reject/reject.css" media="all" />
        <script type="text/javascript" src="https://rawgit.com/codefucker/finalReject/master/reject/reject.min.js"></script>
        <![endif]-->
        <!--[if lt IE 9]>
        <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    {% endblock %}
</head>
<body>
{% block social %}
    <div id="fb-root"></div>
    <script>(function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/ru_RU/sdk.js#xfbml=1&version=v2.7&appId=1756332607957995";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));</script>
{% endblock %}
<div class="global-wrapper">
    {% block header %}
        <!-- HEADER -->
        <header>
            <div class="header-wrap">
                <div class="mbox">
                    <div class="con">
                        <div class="logo-block">
                            {% if logo.image is defined %}
                                <div class="logo">
                                    <img src="{{ logo.image }}" alt="">
                                </div>
                            {% endif %}
                            {% if logo.text is defined %}
                                <div class="logo-text">{{ logo.text }}</div>
                            {% endif %}
                        </div>
                        <div class="nav-part">
                            {% if main_menu is defined and main_menu is not empty %}
                            <ul>
                                {% for menu in main_menu %}
                                <li>
                                    <a href="{{ menu.link }}">{{ menu.title }}</a>
                                </li>
                                {% endfor %}
                            </ul>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!-- /HEADER -->
    {% endblock %}
    <!-- MAIN -->
    <div class="main">
        <!-- add partials here -->

        <div class="banner-wrap">
            <div class="contein" style="background-image: url();">
                <div class="contein-page">
                    <div class="pocke">
