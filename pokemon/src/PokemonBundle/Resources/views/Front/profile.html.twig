{% extends 'PokemonBundle:Front:Base.html.twig' %}

{% block content %}
    <script>
        var pokemon = [
            {% if pokemon is defined and pokemon is not empty %}
            {% for item in pokemon %}
            {
                id: {{ item.id }},
                name: '{{ item.name }}',
                image: '{{ item.image }}'
            },
            {% endfor %}
            {% endif %}
        ];

        var user_points = [
            {% if user_points is defined and user_points is not empty %}
            {% for point in user_points %}
            {
                id: {{ point.id }},
                locationX: {{ point.locationX }},
                locationY: {{ point.locationY }},
                pokemon: {{ point.pokemon.id }},
                jsonInfo: '{{ point.jsonInfo }}',
                confirm: {{ point.confirm }},
                enabled: {{ point.enabled }}
            }{{ (loop.last?'':',') }}
            {% endfor %}
            {% endif %}
        ];
    </script>
